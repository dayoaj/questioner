  },
  "homepage": "https://dayoaj.github.io/questioner/UI",
  "dependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "express-validator": "^5.3.1",
    "moment": "^2.23.0",
    "morgan": "^1.9.1",
    "uuid": "^3.3.2"
  },
